{
  "name": "web-strterkit",
  "version": "1.0.0",
  "description": "web-strterkit",
  "main": "server.js",
  "scripts": {
    "start": "npm run clean:dev && webpack-dev-server --mode development --config webpack.config.dev.js",
    "deploy": "npm run clean && webpack --mode production --config webpack.config.prod.js",
    "deploy:dev": "npm run clean:dev && webpack --mode development --config webpack.config.dev.js",
    "clean": "rimraf public/*",
    "clean:dev": "rimraf dev/*",
    "server": "node server.js",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch"
  },
  "author": "Sutee S.",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.7",
    "jest": "^23.6.0",
    "jest-junit": "^3.6.0",
    "mini-css-extract-plugin": "^0.2.0",
    "mock-local-storage": "^1.0.5",
    "node-sass": "^4.8.3",
    "react-hot-loader": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.5.0",
    "express": "^4.16.3",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-adopt": "^0.6.0",
    "react-dom": "^16.7.0",
    "react-portal": "^3.1.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "reselect": "^3.0.1",
    "rxjs": "^6.2.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|svg|gif)$": "<rootDir>/jest/fileMock.js",
      "^.+\\.(css|scss)$": "<rootDir>/jest/styleMock.js"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "(.mock)\\.(ts|tsx|js)$",
      "/distribution/.*\\.(ts|js)$"
    ],
    "setupTestFrameworkScriptFile": "mock-local-storage",
    "setupFiles": [
      "<rootDir>/jest/global-config-mock.js"
    ]
  }
}
